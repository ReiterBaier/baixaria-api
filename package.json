{
  "name": "baixaria-api",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "cross-env NODE_ENV=prod node ./dist/index.js",
    "worker": "npx ts-node src/worker.ts",
    "lint": "eslint src --max-warnings=0",
    "lint:fix": "eslint src --fix",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate ./migrations/migrations -d ./dist/configs/connection.js",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./dist/configs/connection.js",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./dist/configs/connection.js"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.6",
    "bull": "^4.16.5",
    "bull-board": "^2.1.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "class-validator-jsonschema": "^5.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "nodemon": "^3.0.1",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.4",
    "routing-controllers-openapi": "^4.0.0",
    "ts-node": "^10.9.1",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  }
}
